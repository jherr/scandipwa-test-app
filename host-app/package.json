{
    "name": "scandipwa-test-app-2",
    "description": "Blank app source-code used by CSA.",
    "version": "0.0.0",
    "scripts": {
        "start": "scandipwa-scripts start",
        "build": "scandipwa-scripts build"
    },
    "dependencies": {
        "@scandipwa/scandipwa-scripts": "2.0.12",
        "@scandipwa/framework": "0.0.3",
        "@scandipwa/router": "0.0.3",
        "extension": "link:../extension"
    },
    "scandipwa": {
        "themeAlias": "Blank",
        "type": "theme",
        "extensions": {
            "@scandipwa/framework": true,
            "@scandipwa/router": true,
            "extension": true
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "ignorePatterns": ["src/**/*"],
        "extends": "@scandipwa"
    },
    "license": "OSL-3.0",
    "publishConfig": {
        "access": "public"
    }
}